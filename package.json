{
  "name": "medle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ./.env.dev next dev",
    "build": "node -r dotenv/config node_modules/next/dist/bin/next build",
    "start": "node -r dotenv/config node_modules/next/dist/bin/next start",
    "test": "jest",
    "lint": "eslint .",
    "migrate:make": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:make name_migration",
    "migrate:latest": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:latest",
    "migrate:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:rollback",
    "migrate:list": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:list",
    "seed:make": "node -r dotenv/config ./node_modules/knex/bin/cli seed:make name_seed",
    "seed:run": "node -r dotenv/config ./node_modules/knex/bin/cli seed:run"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@mdx-js/loader": "^1.5.8",
    "@next/mdx": "^9.3.3",
    "@sentry/browser": "^5.12.4",
    "@sentry/integrations": "^5.12.4",
    "@sentry/node": "^5.12.4",
    "@socialgouv/bootstrap.core": "^0.2.0",
    "@socialgouv/jours-feries": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.11",
    "micro-cors": "^0.1.1",
    "moize": "^5.4.5",
    "moment": "^2.24.0",
    "next": "^9.3.0",
    "next-connect": "^0.6.0",
    "next-cookies": "^2.0.3",
    "pg": "^7.12.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-select": "^3.0.8",
    "react-switch": "^5.0.1",
    "reactstrap": "^8.0.1",
    "recharts": "^2.0.0-beta.1",
    "remark-emoji": "^2.1.0",
    "remark-images": "^2.0.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-react": "^0.17.0",
    "@socialgouv/eslint-config-recommended": "^0.17.0",
    "@testing-library/react": "^10.0.0",
    "babel-jest": "^25.1.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.5.1",
    "file-loader": "^5.1.0",
    "jest-cli": "^25.1.0",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^3.1.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "url-loader": "^3.0.0"
  }
}

{
    "name": "medle",
    "version": "1.22.4",
    "private": true,
    "scripts": {
        "seal-secrets": "sre-secrets --to=./.socialgouv",
        "dev": "node scripts/server.js",
        "build": "NODE_ENV=production node -r dotenv/config node_modules/next/dist/bin/next build",
        "debug": "NODE_OPTIONS='--inspect' node -r dotenv/config node_modules/next/dist/bin/next dev",
        "start": "yarn migrate:latest && NODE_ENV=production node scripts/server.js",
        "test": "node -r dotenv/config node_modules/jest-cli/bin/jest --config ./src/__tests__/jest.config.js",
        "test:e2e": "node -r dotenv/config node_modules/jest-cli/bin/jest --config ./src/__e2e__/jest.config.js --runInBand",
        "lint": "eslint ./src",
        "migrate:make": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:make name_migration",
        "migrate:latest": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:latest",
        "migrate:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:rollback",
        "migrate:list": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:list",
        "seed:make": "node -r dotenv/config ./node_modules/knex/bin/cli seed:make name_seed",
        "seed:run:dev": "node -r dotenv/config ./node_modules/knex/bin/cli seed:run --env development",
        "seed:run:staging": "node -r dotenv/config ./node_modules/knex/bin/cli seed:run --env staging",
        "fresh-install": "rm -rf node_modules/ .next/ && yarn"
    },
    "dependencies": {
        "@hookform/resolvers": "^2.8.0",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@mdx-js/loader": "^1.6.22",
        "@next/mdx": "^10.2.3",
        "@sentry/browser": "^6.11.0",
        "@sentry/integrations": "^6.11.0",
        "@sentry/node": "^6.11.0",
        "@socialgouv/bootstrap.core": "^0.2.1",
        "@socialgouv/jours-feries": "^2.0.0",
        "bcryptjs": "^2.4.3",
        "dotenv": "^10.0.0",
        "exceljs": "^4.3.0",
        "express": "^4.18.2",
        "file-saver": "^2.0.5",
        "formidable": "^2.0.1",
        "isomorphic-unfetch": "^3.1.0",
        "js-cookie": "^3.0.0",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.10",
        "knex-upsert": "^0.0.4",
        "micro-cors": "^0.1.1",
        "moize": "^6.0.3",
        "moment": "^2.29.1",
        "next": "13.2.4",
        "next-connect": "^0.10.2",
        "next-cookies": "^2.0.3",
        "node-cron": "^3.0.2",
        "nodemailer": "^6.6.3",
        "normalize-diacritics-es": "^1.0.8",
        "pg": "^8.7.1",
        "pg-query-stream": "^4.5.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.13.0",
        "react-select": "^4.3.1",
        "react-switch": "^6.0.0",
        "reactstrap": "^8.9.0",
        "recharts": "^2.1.16",
        "remark-emoji": "^2.2.0",
        "remark-images": "^2.0.0",
        "styled-components": "^5.3.1",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@jackfranklin/test-data-bot": "^1.3.0",
        "@socialgouv/sre-secrets": "^1.10.4",
        "@testing-library/dom": "^8.1.1",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/user-event": "^13.2.1",
        "babel-jest": "^27.0.6",
        "eslint": "^7.32.0",
        "eslint-config-next": "^13.0.2",
        "faker": "^5.5.3",
        "file-loader": "^6.2.0",
        "jest": "^27.0.6",
        "jest-cli": "^27.0.6",
        "msw": "^0.35.0",
        "prettier": "^2.3.2",
        "prop-types": "^15.7.2",
        "react-test-renderer": "^17.0.2",
        "url-loader": "^4.1.1"
    }
}
